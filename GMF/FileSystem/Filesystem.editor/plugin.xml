<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated My -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="bigkel.presentation.BigkelModelWizardID"
            name="%_UI_BigkelModelWizard_label"
            class="bigkel.presentation.BigkelModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/BigkelModelFile.gif">
         <description>%_UI_BigkelModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated My -->
      <editor
            id="bigkel.presentation.BigkelEditorID"
            name="%_UI_BigkelEditor_label"
            icon="icons/full/obj16/BigkelModelFile.gif"
            extensions="bigkel"
            class="bigkel.presentation.BigkelEditor"
            contributorClass="bigkel.presentation.BigkelActionBarContributor">
      </editor>
   </extension>

</plugin>
